struct Name
{
    byte* first
    byte* last
}

struct Person
{
    Name name
    int age
}

func update_name(Person* p, byte* fname, byte* lname)
{
    p.name = Name {
        .first = fname,
        .last  = lname
    }
}

func main()
{
    var person = Person {
        .age  = 21
    }

    update_name(&person, 'Sam', 'Horlbeck Olsen')

    puts person.name.last
}

