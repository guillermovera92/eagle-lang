extern func printf(byte* ...) : int

func main()
{
    int i = 1
    int j = 4
    puts i | j

    i = 5
    puts i & j

    i &= 1
    puts i

    i = 5
    j = 3
    puts i ^ j
    i ^= j
    puts i

    printf('%X\n', ~0)

    i = 5
    i <<= 2
    puts i

    i >>= 1
    puts i

    puts i << 5
    puts i >> 1
}

