struct Test {
    int^ x
    Test^ next
}

func main()
{
    Test ^t = new Test
    t->x = new int
    t->x^ = 42

    Test^ head = t

    for int i = 0; i < 10; i = i + 1 {
        Test^ temp = new Test
        t->next = temp;
        temp->x = new int
        temp->x^ = i

        t = temp
    }

    head = nil
}
