static int x
static int z = 10
static byte* text = 'Hello, world!'

func inc() : int
{
    x += 1
    return x
}

func inc2() : int
{
    static int y = -11
    y += 3

    text = 'Nope'

    return y
}

func main()
{
    puts inc()
    puts inc()
    puts inc()

    puts inc2()
    puts inc2()
    puts inc2()

